{
  "name": "tea-taster",
  "description": "Tea Tasting Notes",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "assets": "capacitor-assets generate --iconBackgroundColor '#f1ebe1' --splashBackgroundColor '#f1ebe1' --iconBackgroundColorDark '#110b00' --splashBackgroundColorDark '#110b00'",
    "build": "vue-tsc && vite build && cap sync",
    "dev": "vite",
    "lint": "eslint",
    "prepare": "husky install",
    "preview": "vite preview",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "test:dev": "vitest",
    "test:e2e": "cypress run"
  },
  "dependencies": {
    "@capacitor/android": "4.7.3",
    "@capacitor/app": "4.1.1",
    "@capacitor/core": "4.7.3",
    "@capacitor/haptics": "4.1.0",
    "@capacitor/ios": "4.7.3",
    "@capacitor/keyboard": "4.1.1",
    "@capacitor/status-bar": "4.1.1",
    "@ionic/vue": "^7.0.0",
    "@ionic/vue-router": "^7.0.0",
    "ionicons": "^7.0.0",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@capacitor/assets": "~2.0.4",
    "@capacitor/cli": "4.7.3",
    "@ionic/prettier-config": "~2.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "~0.29.8",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.0",
    "cypress": "^12.7.0",
    "eslint": "^8.35.0",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "~8.0.3",
    "jsdom": "^21.1.0",
    "prettier": "~2.8.7",
    "pretty-quick": "~3.1.3",
    "typescript": "~5.0.3",
    "vite": "^4.1.0",
    "vitest": "^0.29.2",
    "vue-tsc": "^1.0.24"
  },
  "prettier": "@ionic/prettier-config"
}
